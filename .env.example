DATABASE_URL=postgresql://user:password@localhost:5432/mydb?schema=public
NODE_ENV=development
AUTH_JWT_SECRET=replace-with-long-random-secret
# Optional JWT issuer/audience for verification context
AUTH_JWT_ISSUER=your-app
AUTH_JWT_AUDIENCE=your-audience
 
# Rate limiting: set to 'true' to disable globally (e.g., local dev/tests)
RATE_LIMIT_DISABLED=false

# Infra backends (optional; defaults to memory)
# CACHE_BACKEND=memory
# QUEUE_BACKEND=memory
# JOBS_BACKEND=memory
# LOCK_BACKEND=memory

# Redis connection (used by cache/queue/jobs when enabled)
# REDIS_URL=redis://localhost:6379
# Shared queue name when using BullMQ
# JOBS_QUEUE_NAME=app:jobs

# SQS (for JOBS_BACKEND=sqs)
# AWS_REGION=us-east-1
# SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/app-jobs.fifo
# SQS_FIFO=true
# SQS_MESSAGE_GROUP_ID=app-jobs
# SQS_CONCURRENCY=5
# SQS_HEARTBEAT_SEC=20
# SQS_EXTEND_BY_SEC=30

# Enable example scheduler in users module (optional)
# EXAMPLE_SCHEDULER_ENABLED=false
