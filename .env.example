DATABASE_URL=postgresql://user:password@localhost:5432/mydb?schema=public
NODE_ENV=development
AUTH_JWT_SECRET=replace-with-long-random-secret
# Optional JWT issuer/audience for verification context
AUTH_JWT_ISSUER=your-app
AUTH_JWT_AUDIENCE=your-audience
\n+# Rate limiting: set to 'true' to disable globally (e.g., local dev/tests)
RATE_LIMIT_DISABLED=false
