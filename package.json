{
  "name": "nextjs-boilerplate",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.18.3",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:arch": "eslint app/**/route.{ts,tsx} modules/**/http/**/*.{ts,tsx}",
    "lint:fix": "eslint . --fix",
    "lint:ci": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prisma:collect": "tsx scripts/prisma-collect.ts",
    "prisma:migrate": "pnpm prisma:collect && prisma migrate dev --schema prisma",
    "prisma:generate": "pnpm prisma:collect && prisma generate --schema prisma",
    "prisma:studio": "prisma studio --schema prisma",
    "prisma:reset": "pnpm prisma:collect && prisma migrate reset --schema prisma --skip-generate --skip-seed",
    "prisma:push": "pnpm prisma:collect && prisma db push --schema prisma",
    "db:prepare": "pnpm prisma:migrate && pnpm prisma:generate",
    "db:seed": "tsx scripts/seed-runner.ts",
    "db:seed:dev": "NODE_ENV=development tsx scripts/seed-runner.ts --tags dev",
    "db:reset": "pnpm prisma:reset && pnpm db:seed",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "playwright test",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "PWDEBUG=1 playwright test",
    "e2e:install": "playwright install --with-deps",
    "check": "pnpm lint && pnpm typecheck && pnpm test",
    "analyze": "ANALYZE=true next build",
    "prepare": "husky install",
    "jobs:stats": "tsx scripts/jobs-stats.ts",
    "jobs:cancel": "tsx scripts/jobs-cancel.ts",
    "jobs:stats:sqs": "tsx scripts/jobs-stats-sqs.ts",
    "module:generate": "tsx scripts/module-generate.ts"
  },
  "prisma": {
    "seed": "tsx scripts/seed-runner.ts"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.910.0",
    "@prisma/client": "^6.7.0",
    "bullmq": "^5.61.0",
    "ioredis": "^5.8.1",
    "jose": "^5.2.0",
    "next": "15.2.0",
    "pino": "^9.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redis": "^5.8.3",
    "scheduler": "0.23.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.0.0",
    "@next/eslint-plugin-next": "15.2.0",
    "@playwright/test": "^1.48.0",
    "@types/node": "^20.11.30",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@types/scheduler": "0.16.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^2.1.9",
    "autoprefixer": "^10.4.19",
    "@faker-js/faker": "^9.2.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import-x": "^4.16.1",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "prisma": "^6.7.0",
    "tailwindcss": "^3.4.9",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5",
    "eslint-plugin-boundaries": "^4.2.1"
  },
  "engines": {
    "node": ">=20 <23"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
